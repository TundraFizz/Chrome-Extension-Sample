{
  "type": "object",

  // "properties" maps an optional key of this object to its schema. At the
  // top-level object, these keys are the policy names supported.
  "properties": {
    "myInt": {
      "type": "integer"
    },

    "myString": {
      "type": "string"
    },

    "myArray": {
      "type": "array",
      "items": {
        "type": "integer"
      }
    }
  }
}
